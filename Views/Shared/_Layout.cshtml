    <script src="https://ajax.aspnetcdn.com/ajax/knockout/3.1.0/knockout-3.1.0.js"></script>
</head>
<body>
        <header>
            <nav class="navbar mb-3">
                <div class="container-fluid">
                    <div class="left-section">
                    <img src="data:image/jpeg;base64,   " alt="Logo" class="logo" /> <!-- Replace with your logo path -->
                        <h1 class="site-title">RTO</h1>
                    </div>
                    @* <div class="center-section">
                        <p class="nav-text">Your first paragraph here</p>
                        <p class="nav-text">Your second paragraph here</p>
                    </div> *@
                <div class="right-section">
                    <a href="https://global.eg.dk/" target="_blank" class="github-button">
                        <i class="fa fa-link"></i> <!-- GitHub icon -->
                    </a>
                </div>
                </div>
            </nav>
        </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>
    @* <footer>
        <div class="footer-container">
            <div class="left-section">
